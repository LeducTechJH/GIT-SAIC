    <text name="TXT__14" text="ACCIONDERECHOS, RESPONSABILIDADES Y ADVERTENCIAS DE LOS PROGRAMAS" x="65" y="32" width="460" height="12"/>
    <text name="TXT__15" text="DE ASISTENCIA NUTRICIONAL (PAN) Y AYUDA TEMPORAL PARA FAMILIAS" x="75" y="48" width="438" height="12"/>
    <text name="TXT__16" text="NECESITADAS (TANF)" x="224" y="64" width="128" height="12"/>
